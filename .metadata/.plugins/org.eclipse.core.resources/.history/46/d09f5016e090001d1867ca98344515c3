package com;

public class LoopingDemo {

	public static void main(String[] args) {
		int i = 0 ;
		int n = 10;
		
		// while(i <= n) {
		//	System.out.println("while" + i);
		//	i++;
		// }
		
		do {
			System.out.println("do while" + i);
			i++;
		} while(i<=n);
		
		System.out.println("finish");

	}

}
